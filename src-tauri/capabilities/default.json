{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"notification:default",
        "fs:allow-create",
        "fs:allow-read-text-file",
        "fs:allow-exists",
        "fs:allow-desktop-read",
        "fs:allow-write",
        "fs:allow-write-text-file"
	]
}
